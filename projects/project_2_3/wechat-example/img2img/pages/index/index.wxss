

.page_img2img{
  position: relative;
  width: 100vw;
  display: flex;
  margin-top: 32rpx;
  margin-bottom: 32px;
  flex-direction: column;
  align-items: center;
}
.upload-button{
  box-sizing: border-box;
  width: 500rpx;
  height: 500rpx;
  line-height: 100px;
  margin-bottom: 10px;
  display: flex;
  background-color: #F6F6F6;
  justify-content: center;
  align-items: center;
  text-align: center;
  font-size: 60px;
  color: #C4C4C4;
  font-weight: 300;
  border-radius: 10px;
  overflow: hidden;

}
.upload-button>view{
  margin-top: -20px;
}